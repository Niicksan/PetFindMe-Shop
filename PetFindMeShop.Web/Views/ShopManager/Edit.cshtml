@model ShopManagerFormModel

@{
    ViewBag.Title = "Редактиране на мениджър профил";
}

<h2 class="text-center">@ViewBag.Title</h2>

@if (!ViewData.ModelState.IsValid && ViewData.ModelState["Error"] != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewData.ModelState["Error"].Errors.First().ErrorMessage
    </div>
}

<partial name="_ShopManagerFormPartial" model="@Model" />

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
